package com.testyantra.infobean;

public class UserCreation {
	String uName;
	int uAge;
	String uEmail;
	String uPassword;
	Long uuMobileNo;
	public String getuName() {
		return uName;
	}
	public void setuName(String uName) {
		this.uName = uName;
	}
	public int getuAge() {
		return uAge;
	}
	public void setuAge(int uAge) {
		this.uAge = uAge;
	}
	public String getuEmail() {
		return uEmail;
	}
	public void setuEmail(String uEmail) {
		this.uEmail = uEmail;
	}
	public String getuPassword() {
		return uPassword;
	}
	public void setuPassword(String uPassword) {
		this.uPassword = uPassword;
	}
	public Long getUuMobileNo() {
		return uuMobileNo;
	}
	public void setUuMobileNo(Long uuMobileNo) {
		this.uuMobileNo = uuMobileNo;
	}




}
